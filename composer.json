{
    "name": "newage/annotations",
    "type": "library",
    "description": "A map of entity generator from annotations on console for Zend Framework 2.",
    "license": "MIT",
    "homepage": "https://github.com/newage/annotations",
    "keywords": [
        "annotations",
        "zendframework"
    ],
    "authors": [
        {
            "name": "Vadim Leontiev",
            "email": "vadim.leontiev@gmail.com",
            "homepage": "https://github.com/newage",
            "role": "Developer"
        }
    ],
    "config": {
        "bin-dir": "bin/"
    },
    "require": {
        "php": ">=5.4",
        "doctrine/common": ">=2.1",
        "zendframework/zendframework": ">=2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Newage\\Annotations\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1"
        }
    },
    "scripts": {
        "cs": "php bin/phpcs src --standard=PSR2 --colors",
        "cs-fix": "php bin/phpcbf src --standard=PSR2 --colors"
    }
}